<h1>Create a Ticket</h1>

</hr>

	<%= form_for [:my, @ticket] do |f| %>

	<div class= "span6">


		<%=f.text_field :description, placeholder: "What do you need help on?", class: "input-block-level" %>
		<div class="form-actions">
			<%=f.submit 'Create Ticket', class: 'btn btn-primary' %>
		</div>
	</div>

<% end %>